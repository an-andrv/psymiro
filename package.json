{
  "name": "psymiro",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --no-cache --colors --detectOpenHandles",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache",
    "test:coverage": "jest --coverage",
    "cypress-component": "ng run psymiro:cypress-component",
    "cypress-e2e": "ng run psymiro:cypress-e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.4",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.4",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@cypress/schematic": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
    "typescript": "~4.8.2"
  }
}
